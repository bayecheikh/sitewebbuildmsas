Options -Indexes
# Security Headers
<IfModule mod_headers.c>
	Header set X-XSS-Protection "1; mode=block"
	Header set X-Frame-Options "SAMEORIGIN"
	Header set X-Content-Type-Options "nosniff"
	Header always set Strict-Transport-Security "max-age=63072000; includeSubDomains"
	Header set Content-Security-Policy "default-src  'self' http: https:; frame-ancestors  'self' http: https:; manifest-src  'self' http: https:; script-src  'self' http: https:; script-src-elem  'self' http: https:; script-src-attr  'self' http: https:; style-src  'self' http: https:; style-src-elem  'self' http: https:; style-src-attr  'self' http: https:; img-src  'self' http: https: data:; connect-src  'self' http: https:; frame-src  'self' http: https:; font-src  'self' http: https:; media-src  'self' http: https:; object-src  'self' http: https:; manifest-src  'self' http: https:; worker-src  'self' http: https:; prefetch-src  'self' http: https:; form-action  'self' http: https:;"
	Header set Referrer-Policy "same-origin"
	Header set Feature-Policy "geolocation  'self' http: https:; vibrate 'none'"
</IfModule>